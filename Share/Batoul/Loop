//Question=>19
 print("Display n terms of odd natural number and their sum ");
  int num1, num2, num3 = 0;
  int num4 = num.parse(stdin.readLineSync());
  for (num1 = 1; num1 <= num4; num1++)
  {
    stdout.write(2 * num1 - 1);
    stdout.write(" ");

    num3+= 2 * num1 - 1;

  }
  print(" ");
  print( "The Sum of odd Natural Numbers up to $num4 is :$num3" );





