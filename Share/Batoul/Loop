//Question=>2
{
  print("pleas enter 2 number");
  int sum=0;
  int num1=num.parse(stdin.readLineSync());
  int num2=num.parse(stdin.readLineSync());
  for(int i =num1+1 ; i<num2; i++)
    { sum=sum+i;

    }
  print (sum);
}





