// Question:1


List a =[1,4,9,16,25,36,49,64,81,100];
List b=[];
print(a);
print("Make a new list that has only the even element of this list");
for (var e in a) {
  if (e % 2 == 0) {
   b.add(e);

  }
}
print(b);





