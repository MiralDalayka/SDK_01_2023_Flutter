   {print("Your bounce =  ${salary*0.15}");}

 else  if (salary<2000 && salary >=1300)
 {print("Your bounce =  ${salary*0.20}");}

 else  if (salary<1300 && salary >=700)
 {print("Your bounce =  ${salary*0.35}");}

 else  if (salary<700)
 {print("Your bounce =  ${salary*0.40}");}











//Question=>14
arithmetic_operations()
{

  print("Pleas enter 2 number");
  num num1=num.parse(stdin.readLineSync());
  num num2=num.parse(stdin.readLineSync());
  print("Pleas enter the operation that you want make it (+ , - , * , / )");
  String operation=stdin.readLineSync();

  switch(operation)
  {
        case '+':
        print("number1 + number2= ${num1+num2}");
        break;


        case '-':
        print("number1 - number2= ${num1-num2}");
        break;

        case '*':
        print("number1 * number2= ${num1*num2}");
        break;

        case '/':
        print("number1 / number2= ${num1/num2}");
        break;

       default:
       print(" Sory,invalid operatin");



  }

}
void main ()
{
  arithmetic_operations();

}













