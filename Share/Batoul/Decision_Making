//Question:7
import 'dart:io';
import 'dart:math';
void main() {

  print("Pleas enter 3 numbers");
  int num1=num.parse(stdin.readLineSync());
  int num2=num.parse(stdin.readLineSync());
  int num3=num.parse(stdin.readLineSync());
  int max_num=max(num1,num2);
  max_num=max(max_num,num3);
  
  print("1st Number=$num1 , 2nd Number=$num2 , 3rd Number=$num3");
  print("The 3rd Number is the greatest among three is :$max_num");
}












  // Question:8
  print("pleas enter temperature");
  dynamic temperature = num.parse(stdin.readLineSync());
  if (temperature < 0) {
    print("Freezing weather");
  }

  else if (temperature >= 0 && temperature < 10) {
    print("Very Cold weather");
  }

  else if (temperature >= 10 && temperature < 20)
   { print("Very Cold weather");}

  else if (temperature>=20 && temperature<30)
   { print("Normal in Temp");}

  else if (temperature>=30 && temperature<40)
   { print("Its Hot");}

  else  if (temperature>=40)
  { print(" Its Very Hot");}









