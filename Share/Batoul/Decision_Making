//Question=>12

  print("input marks of five subjects Physics, Chemistry, Biology, Mathematics and Computer ");
var phy =num.parse(stdin.readLineSync());
var chem = num.parse(stdin.readLineSync());
var bio = num.parse(stdin.readLineSync());
var math = num.parse(stdin.readLineSync());
var comp = num.parse(stdin.readLineSync());

if ((phy + chem + bio +math+ comp)/5>=90 )
  {print("Grade A");}
else if ((phy + chem + bio +math+ comp)/5>=80 &&  (phy + chem + bio +math+ comp)/5<90)
{print("Grade B");}
else if ((phy + chem + bio +math+ comp)/5>=70 && (phy + chem + bio +math+ comp)<80)
{print("Grade C");}
else if ((phy + chem + bio +math+ comp)/5>=60 && (phy + chem + bio +math+ comp)<70)
{print("Grade D");}
else if ((phy + chem + bio +math+ comp)/5>=40 && (phy + chem + bio +math+ comp)<60)
{print("Grade E");}
else if ((phy + chem + bio +math+ comp)/5<40)
{print("Grade F");}












