//Question:12

  print("input marks of five subjects Physics, Chemistry, Biology, Mathematics and Computer ");
  var phy =num.parse(stdin.readLineSync());
  var chem = num.parse(stdin.readLineSync());
  var bio = num.parse(stdin.readLineSync());
  var math = num.parse(stdin.readLineSync());
  var comp = num.parse(stdin.readLineSync());

 var grade=(phy + chem + bio +math+ comp)/5;
  if (grade>=90 )
  {print("Grade A");}
  else if (grade>=80 &&  grade<90)
  {print("Grade B");}
  else if (grade>=70 && grade<80)
  {print("Grade C");}
  else if (grade>=60 && grade<70)
  {print("Grade D");}
  else if (grade>=40 && grade<60)
  {print("Grade E");}
  else if (grade<40)
  {print("Grade F");}







//Question:13

  print("Please enter your salary to calculate your bounce:");
var salary =num.parse(stdin.readLineSync());
dynamic bounce;
 if (salary>=2000)
   {print("Your bounce =  ${salary*0.15}");}

 else  if (salary<2000 && salary >=1300)
 {print("Your bounce =  ${salary*0.20}");}

 else  if (salary<1300 && salary >=700)
 {print("Your bounce =  ${salary*0.35}");}

 else  if (salary<700)
 {print("Your bounce =  ${salary*0.40}");}








//Question:14

operators(){
  print("Menu driven calculator");
  print("1- +");
  print("2- -");
  print("3- *");
  print("4- %");
  print("Pleas enter the operator ane two numbers");
  String operator=stdin.readLineSync();

  int num1=num.parse(stdin.readLineSync());
  int num2=num.parse(stdin.readLineSync());
  switch(operator){
    case '+':
      print("num1+num2=${num1+num2}");
      break;
    case '-':
      print("num1-num2=${num1-num2}");
      break;
    case '*':
      print("num1*num2=${num1*num2}");
      break;
    case '%':
      print("num1%num2=${num1%num2}");
      break;

    default:
      print("Sorry,Error operation ");
      break;

  }
}
void main() {

operators();


}








//Question:15
  print("Pleas enter any alphabet");
  String alphabet=stdin.readLineSync();
  switch(alphabet){
    case 'a':
      print("vowel ");
      break;
    case 'i':
      print("vowel ");
      break;
    case 'o':
      print("vowel ");
      break;
    case 'u':
      print("vowel ");
      break;
    case 'e':
      print("vowel ");
      break;
      default:
        print("constant ");
      break;

  }
