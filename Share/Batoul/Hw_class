  logout() {
    String UserName;
    String Password2;

    print(
        "Please enter your user_name and password ");
    UserName = stdin.readLineSync();
    Password2 = stdin.readLineSync();

    if (Password2 == Password && UserName == UserName) {
      print("LogOut");
    }
    else {
      print("Error in your name or password");
    }
  }
}




class Author
{
String _name;
String _email;
String _gender;

Autho (name, email, gender)
{
  print(
      "Please enter name , email, gender");

  print(
      "Note --> gender must be f or m ");

  _name = stdin.readLineSync();
 _email= stdin.readLineSync();
 _gender = stdin.readLineSync();

}


void set email(em)
{
  _email=em;
}
String get getname
{
  return _name;
}

String get getemail
{
  return _email;
}

String to_string()
{
  if (_gender=='m' || _gender=='f')
    {print("gender:$_gender");}
  else {print("Erorr in gender");}
}

}
